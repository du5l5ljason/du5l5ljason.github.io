   57  // If It's a navigation req and it's url.pathname isn't end with '/' or '.ext'
   58  // it should be a dir/repo request and need to be fixed (a.k.a be redirected)
   59: // Tracking https://twitter.com/Huxpro/status/798816417097224193
   60  const shouldRedirect = (req) => (isNavigationReq(req) && new URL(req.url).pathname.substr(-1) !== "/" && !endWithExtension(req))
